{
  "manifest_version": 2,

  "name": "Twiter Social Toolkit",
  "description": "Share text,images,links,content,pages to u'r twitter account",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Click Here!"
  },
  
  "icons":{
      "64": "icon.png",
      "32": "icon.png",
      "16": "icon.png",
      "128": "icon.png"
    },

  "background":{
    "scripts":["background.js"]     
  },

  "permissions":[
    "contextMenus"
  ]
}

